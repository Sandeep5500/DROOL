int main()
{
    Matrix m1[10][5];

    for (int i=0; i<10; i++)
    {
        for (int j=0; j<5; j++)
        {
            m1[i][j] = i+j;
        }
    }

    output(m1);
    output(det(m1));  //unary expr
    output(inv(m1));
    output(trans(m1));
    m1 addr (1,2,3); //addrc expr
    m1 addc (4,5,6);
    m1 delr;
    m1 delc;   //postfix
} 