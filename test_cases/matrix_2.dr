# matrices 2

int main()
{
    Matrix m1[10][5], m2[10][5];
    
    for (int i=0; i<10; i++)
    {
        for (int j=0; j<5; j++)
        {
            m1[i][j] = i+j;
        }
    }

    for (int i=0; i<10; i++)
    {
        for (int j=0; j<5; j++)
        {
            m2[i][j] = i*j;
        }
    }

    output(m1+m2);
    output(m1-m2);
    output(m1*m2);
}