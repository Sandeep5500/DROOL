Int a = 10, b = 20;

// postfixExpression : postfixExpression (PlusPlus | MinusMinus)
a++;
b--;
print(a + " " + b + "\n");      // output : 11 20

// bitwise logical Operator
print(a | b);       // output : 27
print("\n");

print(a & b);       // output : 3
print("\n");

print(a ^ b);       // output : 24 
print("\n");

print(!a);          // output : 4
print("\n")