class Main inherits IO {
	
	printGraph (Graph G)
	{
		foreach (Vertex v in G)	-- ye shayad dala nahi hai to normal for loop chalana hoga
		{
			print(v + " ");	-- print statement bhi dhoondhna hoga
		}
		print("\n");
		
		Int i = 0;
		foreach (Edge e in G)
		{
			i++;
			print("edge" + i + " " + e);
			print("\n");
		}
	}	
	
	main() : Object 
	{
		-- Vetices 
		Vertex v1, v2, v3, v4, v5, v6;
		
		-- Graph with its vertices bieng pushed
		Graph G << v1, v2, v3, v4 , v5 , v6
		
		-- Edge definitions for the above graph G
		Edge e1 = (v1, v2), e2 = (v1, v4) e3 = (v1, v5);
		Edge e4 = (v2, v6), e5 = (v2, v3);
		Edge e6 = (v4, v5), e7 = (v4, v3), e8 = (v3, v6);
		
		-- pushing the above edges in graph G
		G << e1, e2, e3, e4, e5, e6, e7, e8
		
		-- printing the graph vetices and edges
		printGraph(G);
	}
				
